{
  "name": "@vladdenisov/crove-bot",
  "version": "0.2.0",
  "description": "Simple Bot For Discord",
  "main": "index.js",
  "scripts": {
    "test": "echo At this point, the build has succeded and the bot should've been tested, but I don't want to run the bot from the CI.",
    "lint": "eslint \"{,!(node_modules)/**/}*.{js,jsx}\" --fix",
    "start": "node .",
    "server": "forever start --id \"crove\" index.js",
    "stop": "forever stop --id \"crove\" index.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "bot",
    "discord",
    "music",
    "spotify",
    "youtube",
    "voice",
    "ytdl"
  ],
  "author": "Vlad Denisov",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@logux/eslint-config",
    "rules": {
      "global-require": "off",
      "node/no-unpublished-require": "off",
      "prefer-let/prefer-let": "off",
      "max-len": "off",
      "no-invalid-this": "off"
    },
    "overrides": [],
    "globals": {
      "servers": false
    }
  },
  "dependencies": {
    "@discordjs/opus": "^0.1.0",
    "color-thief-node": "^1.0.3",
    "discord.js": "github:discordjs/discord.js",
    "enmap": "^5.2.4",
    "ffmpeg-static": "^4.0.0",
    "giphy-api": "^2.0.1",
    "ibm-watson": "^5.4.0",
    "node-fetch": "^2.6.0",
    "node-opus": "^0.3.3",
    "spotify-web-api-node": "^4.0.0",
    "yargs-parser": "^13.1.2",
    "ytdl-core": "^2.0.1",
    "ytpl": "^0.1.20",
    "ytsr": "github:vladdenisov/node-ytsr",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@logux/eslint-config": "^36.1.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^18.0.1",
    "forever": "^2.0.0"
  },
  "snyk": true
}
